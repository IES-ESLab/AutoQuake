{
    "LoggingConfig":{
        "version": 1,
        "disable_existing_loggers": false,
        "formatters": {
          "default": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
          }
        },
        "handlers": {
          "console": {
            "class": "logging.StreamHandler",
            "formatter": "default",
            "level": "INFO"
          },
          "file": {
            "class": "logging.FileHandler",
            "formatter": "default",
            "filename": "logging.log",
            "level": "DEBUG"
          }
        },
        "root": {
          "handlers": ["console", "file"],
          "level": "DEBUG"
        }
    }
}